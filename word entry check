pyg = 'ay'

original = raw_input('Enter a word:')
word = original.lower()
first = word[0]

if len(original) > 0 and original.isalpha():
        if (first == 'a') or (first == 'e') or (first
                                  == 'i')or(first == 'o')or (first == 'u'):
            print "vowel"
        else:
            print "consonant"
        print original
else:
        print 'empty'
